{
    "name": "bkief29/laravel-dto",
    "type": "library",
    "description": "Data Transfer Objects complete with castable attributes and validation.",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Kiefer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": ">=6",
        "spatie/data-transfer-object": "^1"
    },
    "require-dev": {
        "orchestra/testbench": "5.x-dev",
        "phpunit/phpunit": "^8"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "bkief29\\DTO\\DataTransferObjectServiceProvider"
            ],
            "aliases": {
                "DTO": "bkief29\\DTO\\Facades\\DTO"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "bkief29\\DTO\\": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "bkief29\\DTO\\Tests\\": "tests"
      }
    },
    "scripts": {
      "post-autoload-dump": [
        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
      ]
    },
    "minimum-stability": "dev"
}
